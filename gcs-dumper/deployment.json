{
    "kind": "Deployment",
    "apiVersion": "extensions/v1beta1",
    "metadata": {
        "name": "reddit-dumber",
        "namespace": "default",
        "labels": {
            "run": "reddit-dumper"
        },
        "annotations": {
            "deployment.kubernetes.io/revision": "1"
        }
    },
    "spec": {
        "replicas": 1,
        "selector": {
            "matchLabels": {
                "run": "reddit-dumper"
            }
        },
        "template": {
            "metadata": {
                "creationTimestamp": null,
                "labels": {
                    "run": "reddit-dumper"
                }
            },
            "spec": {
                "containers": [
                    {
                        "name": "reddit-dumper",
                        "image": "gcr.io/hillaryvdonald/gcs-dumper:v5",
                        "resources": {},
                        "terminationMessagePath": "/dev/termination-log",
                        "imagePullPolicy": "IfNotPresent",
                        "env" : [
                          {
                            "name":"BUCKET",
                            "value":"reddit-db"
                          },
                          {
                            "name":"EXCHANGE",
                            "value":"reddit"
                          }
                        ]
                    }
                ],
                "restartPolicy": "Always",
                "terminationGracePeriodSeconds": 30,
                "dnsPolicy": "ClusterFirst",
                "securityContext": {}
            }
        },
        "strategy": {
            "type": "RollingUpdate",
            "rollingUpdate": {
                "maxUnavailable": 1,
                "maxSurge": 1
            }
        }
    },
    "status": {
        "observedGeneration": 2,
        "replicas": 1,
        "updatedReplicas": 1,
        "availableReplicas": 1
    }
}
