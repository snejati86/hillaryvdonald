{
   "kind":"ReplicationController",
   "apiVersion":"v1",
   "metadata":{
      "name":"server-rc",
      "labels":{
         "name":"server",
         "app":"server"
      }
   },
   "spec":{
      "replicas":3,
      "selector":{
         "name":"server"
      },
      "template":{
         "metadata":{
            "labels":{
               "name":"server",
               "app":"server"
            }
         },
         "spec":{
            "containers":[
               {
                  "name":"server",
                  "image":"gcr.io/hillaryvdonald/server:v12",
                  "ports":[
                     {
                        "name":"http-server",
                        "containerPort":8001
                     }
                  ]
               }
            ]
         }
      }
   }
}
